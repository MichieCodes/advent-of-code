 {
  "configurations": {
    "run": {
      "adapter": "vscode-node",
      "breakpoints": {
        "exception": {
          "all": "N",
          "uncaught": "N"
        }
      },
      "configuration": {
        "request": "launch",
        "protocol": "auto",
        "runtimeArgs": ["-r", "ts-node/register"],
        "args": ["${workspaceRoot}/part2.ts"],
        "stopOnEntry": true,
        "console": "integratedTerminal",
        "cwd": "${workspaceRoot}"
      }
    }
  }
}
